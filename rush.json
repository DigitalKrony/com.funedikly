{
  "$schema": "https://developer.microsoft.com/json-schemas/rush/v5/rush.schema.json",
  "rushVersion": "5.150.0",
  "yarnVersion": "1.22.22",
  "ensureConsistentVersions": true,
  "projectFolderMinDepth": 2,
  "projectFolderMaxDepth": 4,
  "approvedPackagesPolicy": {
    "reviewCategories": [
      "production",
      "pre-production",
      "prototype",
      "tools"
    ],
    "ignoredNpmScopes": [
      "@types"
    ]
  },
  "repository": {
    "url": "https://github.com/DigitalKrony/com.funedikly"
  },
  "eventHooks": {
    "preRushInstall": [],
    "postRushInstall": [],
    "preRushBuild": [],
    "postRushBuild": []
  },
  "projects": [
    {
      "packageName": "@df/dux",
      "projectFolder": "Packages/DUX",
      "reviewCategory": "production",
      "shouldPublish": true
    },
    {
      "packageName": "@df/utilities",
      "projectFolder": "Packages/DFUtilities",
      "reviewCategory": "tools",
      "shouldPublish": false
    },
    {
      "packageName": "@df/static-db",
      "projectFolder": "Packages/StaticDB",
      "reviewCategory": "tools",
      "shouldPublish": false
    }
  ]
}